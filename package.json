{
  "private": true,
  "name": "zero-base-manual-dev-config",
  "version": "1.0.0",
  "description": "프론트엔드 개발에 대해 학습합니다.",
  "config": {
    "dir": "./public/scripts ./server",
    "path": "./public/scripts/**/*.js ./server/**/*.js"
  },
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "nodemon server/app.js --port=4200",
    "lint": "eslint $npm_package_config_path --ignore-path .gitignore",
    "watch:lint": "",
    "format": "",
    "watch:format": "",
    "preview": "node server/app.js"
  },
  "keywords": [
    "ecmascript",
    "eslint",
    "prettier",
    "jest"
  ],
  "author": "yamoo9 <yamoo9@euid.dev>",
  "devDependencies": {
    "eslint": "8.26.0",
    "live-server": "1.2.2",
    "nodemon": "2.0.20"
  }
}