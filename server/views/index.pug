doctype html
html(lang='ko-KR')
  head
    meta(charset="UTF-8")
    title= title
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet", href="/style.css")
  body
    h1= headline
    nav 
      ul
        each linkItem in linkList
          li
            a(href= linkItem.href)= linkItem.text